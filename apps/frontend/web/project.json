{
  "name": "frontend-web",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/frontend/web",
  "projectType": "application",
  "tags": ["language:typescript", "layer:frontend", "scope:web"],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/frontend/web"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/frontend/web"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "dev": true,
        "buildTarget": "frontend-web:build",
        "proxyConfig": "apps/frontend/web/proxy.conf.json"
      },
      "configurations": {
        "development": {
          "dev": true,
          "buildTarget": "frontend-web:build:development"
        },
        "production": {
          "dev": false,
          "buildTarget": "frontend-web:build:production"
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "frontend-web:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "{projectRoot}/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  }
}
