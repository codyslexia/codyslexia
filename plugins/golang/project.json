{
  "name": "@plugins/golang",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "plugins/golang/src",
  "projectType": "library",
  "tags": ["language:typescript", "layer:isomorphic", "scope:shared", "type:tool"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/plugins/golang",
        "main": "plugins/golang/src/index.ts",
        "tsConfig": "plugins/golang/tsconfig.lib.json",
        "assets": [
          "plugins/golang/*.md",
          {
            "input": "./plugins/golang/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./plugins/golang/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./plugins/golang",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./plugins/golang",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "{projectRoot}/jest.config.ts"
      }
    }
  }
}
